<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"  
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<settings>
        <!-- 是否开启全局缓存 -->
        <setting name="cacheEnabled" value="true"/>
        <!-- 查询时，关闭关联对象即时加载以提高性能 -->
        <setting name="lazyLoadingEnabled" value="false"/>
        <!-- 对于未知的SQL查询，允许返回不同的结果集以达到通用的效果 -->
        <setting name="multipleResultSetsEnabled" value="true"/>
        <!-- 设置关联对象加载的形态，此处为按需加载字段(加载字段由SQL指 定)，不会加载关联表的所有字段，以提高性能 -->
        <setting name="aggressiveLazyLoading" value="true"/>
    </settings>
    
    <typeAliases>
        <typeAlias type="com.hnqj.core.PageData" alias="pd"/>
        <typeAlias type="com.hnqj.model.Account" alias="Account"/>
        <typeAlias type="com.hnqj.model.Advert" alias="Advert"/>
        <typeAlias type="com.hnqj.model.Cashrecord" alias="Cashrecord"/>
        <typeAlias type="com.hnqj.model.Changelog" alias="Changelog"/>
        <typeAlias type="com.hnqj.model.Client" alias="Client"/>
        <typeAlias type="com.hnqj.model.Collection" alias="Collections"/>
        <typeAlias type="com.hnqj.model.Comment" alias="Comment"/>
        <typeAlias type="com.hnqj.model.Dealrecord" alias="Dealrecord"/>
        <typeAlias type="com.hnqj.model.Dict" alias="Dict"/>
        <typeAlias type="com.hnqj.model.Distribution" alias="Distribution"/>
        <typeAlias type="com.hnqj.model.Findimg" alias="Findimg"/>
        <typeAlias type="com.hnqj.model.Groomapply" alias="Groomapply"/>
        <typeAlias type="com.hnqj.model.Merch" alias="Merch"/>
        <typeAlias type="com.hnqj.model.Merchdown" alias="Merchdown"/>
        <typeAlias type="com.hnqj.model.Modules" alias="Modules"/>
        <typeAlias type="com.hnqj.model.Playimg" alias="Playimg"/>
        <typeAlias type="com.hnqj.model.Releaseadvert" alias="Releaseadvert"/>
        <typeAlias type="com.hnqj.model.Roles" alias="Roles"/>
        <typeAlias type="com.hnqj.model.Rolesmodules" alias="Rolesmodules"/>
        <typeAlias type="com.hnqj.model.Rolesuser" alias="Rolesuser"/>
        <typeAlias type="com.hnqj.model.Sysconfig" alias="Sysconfig"/>
        <typeAlias type="com.hnqj.model.Syslog" alias="Syslog"/>
        <typeAlias type="com.hnqj.model.Sysusermgr" alias="Sysusermgr"/>
        <typeAlias type="com.hnqj.model.Template" alias="Template"/>
        <typeAlias type="com.hnqj.model.Train" alias="Train"/>
        <typeAlias type="com.hnqj.model.Unsubscribe" alias="Unsubscribe"/>
        <typeAlias type="com.hnqj.model.Userinfo" alias="Userinfo"/>
        <typeAlias type="com.hnqj.model.Works" alias="Works"/>
        <typeAlias type="com.hnqj.model.Integral" alias="Integral"/>
        <typeAlias type="com.hnqj.model.Limit" alias="Limit"/>
        <typeAlias type="com.hnqj.model.Proportions" alias="Proportions"/>
        <typeAlias type="com.hnqj.model.Merchcycle" alias="Merchcycle"/>
    </typeAliases>
</configuration>